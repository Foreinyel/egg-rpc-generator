{
  "name": "@hemyn/egg-rpc-generator",
  "version": "0.0.2",
  "description": "RPC tools for egg framework",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "egg-rpc-generator": "bin/egg-rpc-generator"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint . --ext .js",
    "cov": "TEST_TIMEOUT=30000 egg-bin cov",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "pkgfiles": "egg-bin pkgfiles --check",
    "ci": "npm run autod -- --check && npm run pkgfiles && npm run lint && npm run cov",
    "contributors": "contributors -f plain -o AUTHORS",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Foreinyel/egg-rpc-generator.git"
  },
  "keywords": [
    "egg",
    "rpc",
    "tool"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Foreinyel/egg-rpc-generator#readme",
  "dependencies": {
    "antpb": "^1.0.0",
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "egg-utils": "^2.4.1",
    "handlebars": "^4.0.12",
    "interop-require": "^1.0.0",
    "is-type-of": "^1.2.1",
    "jar2proxy": "^1.0.3",
    "jsdoc": "^3.5.5",
    "maven": "^4.4.1",
    "mkdirp": "^0.5.1",
    "mz": "^2.7.0",
    "mz-modules": "^2.1.0",
    "nunjucks": "^3.1.7",
    "rimraf": "^2.6.3",
    "sdk-base": "^3.5.1",
    "standard-version": "^9.3.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "contributors": "^0.5.1",
    "egg": "^2.14.2",
    "egg-bin": "^4.10.0",
    "egg-ci": "^1.11.0",
    "eslint": "^5.12.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "ci": {
    "type": "travis",
    "version": "8, 10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
